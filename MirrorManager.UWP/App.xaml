<Application
    x:Class="MirrorManager.UWP.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:MirrorManager.UWP"
    xmlns:viewModel="using:MirrorManager.UWP.ViewModels"
    RequestedTheme="Light">

    <Application.Resources>
        <x:String x:Key="ida:ClientID"></x:String>
        <x:String x:Key="ida:AADInstance">https://login.microsoftonline.com/</x:String>
        <x:String x:Key="ida:Domain">microsoft.onmicrosoft.com</x:String>

        <x:String x:Key="FaceApiKey"></x:String>

        <viewModel:ViewModelLocator x:Key="ViewModelLocator" />
    </Application.Resources>
    
</Application>
